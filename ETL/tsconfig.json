{
  "compilerOptions": {
    "target": "es2022",
    "outDir": "target",
    "sourceMap": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "moduleResolution": "Node",
    "module": "ES2022",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "ES2022"
    ]
  }
}